{
  "name": "godjira",
  "version": "0.1.0",
  "private": true,
  "main": "./desktop-electron-app/main.js",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "kill": "pkill -f node",
    "types:auto-add": "typesync || :",
    "types:auto-check": "npm run types:auto-add -- --dry",
    "desktop:dev": "concurrently -n \"NEXT,ELECTRON\" -c \"yellow,blue\" --kill-others \"next dev\" \"electron .\"",
    "desktop:build": "next build && electron-builder",
    "eslint:format": "eslint src --fix",
    "prettier:format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,mdx,css,yaml,yml,json}\"",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,js,jsx,md,mdx,css,yaml,yml,json}\"",
    "lint": "next lint",
    "test": "jest --coverage",
    "test:ci": "jest --ci",
    "test:watch": "jest --watchAll",
    "prepare": "husky",
    "postinstall": "husky install",
    "prisma:migrate-dev": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:tidy": "prisma format",
    "prisma:generate": "prisma generate",
    "prisma:seed": "prisma db seed"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.29.9",
    "@next-auth/prisma-adapter": "0.4.4-canary.81",
    "@prisma/client": "^5.10.2",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-slot": "^1.0.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "electron-serve": "^1.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lucide-react": "^0.344.0",
    "million": "^2.6.0-beta.11",
    "next": "canary",
    "next-auth": "^4.24.7",
    "next-theme": "^0.1.5",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.1",
    "tailwind-variants": "^0.2.0",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.1.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/autoprefixer": "~10.2.0",
    "@types/eslint": "~8.56.5",
    "@types/eslint-config-prettier": "~6.11.3",
    "@types/lint-staged": "~13.3.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "electron": "^29.0.1",
    "electron-builder": "24.12.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "husky": "^9.0.11",
    "lint-staged": "15.2.2",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "0.5.11",
    "prisma": "^5.10.2",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "typesync": "^0.12.1"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn prettier:format",
      "yarn eslint:format"
    ],
    "*.js": "eslint --cache --fix"
  },
  "packageManager": "yarn@4.1.0"
}
